			  $("#qlocation").val(parsedJson.location);
			  $("#qrecipientid").val(parsedJson.recipientid);
			  $("#qtransferAmount").val(parsedJson.transferAmount);
			  $("#qenergyTransactionAmount").val(parsedJson.energyTransactionAmount);

			  //DetermineCostdata
			  $("#date").val(parsedJson.date);
		      $("#chargecarcost").val(parsedJson.chargecarcost);
			  $("#procsellcost").val(parsedJson.procsellcost);
			  $("#cmlxsellcost").val(parsedJson.cmlxsellcost);
